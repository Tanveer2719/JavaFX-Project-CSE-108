<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="293.0" prefWidth="464.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FX.LoginController">
   <children>
      <Label layoutX="100.0" layoutY="116.0" text="Username" />
      <TextField fx:id="userText" layoutX="197.0" layoutY="112.0" />
      <ButtonBar layoutX="172.0" layoutY="193.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetAction" text="Reset" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginAction" text="Login" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
